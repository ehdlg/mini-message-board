<!DOCTYPE html>
<html>
  <%- include('./partials/head.ejs') %>

  <body>
    <%-include('./partials/header-nav.ejs') %>
    <main>
      <div id="messages">
        <% messages.forEach(message => { %>
        <div class="message">
          <h3><%= message.user %></h3>
          <p><%= message.text %></p>
          <p class="message-date">Added: <%= message.added %></p>
        </div>
        <% }) %>
      </div>
    </main>
  </body>
</html>
